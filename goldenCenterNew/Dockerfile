# Stage 1: BuildFROM mcr.microsoft.com/dotnet/sdk:8.0 AS buildWORKDIR /srcCOPY *.csproj ./RUN dotnet restore "./goldenCenterNew.csproj"COPY . .RUN dotnet build "./goldenCenterNew.csproj" -c Release -o /app/build# Stage 2: PublishFROM build AS publishRUN dotnet publish "./goldenCenterNew.csproj" -c Release -o /app/publish# Stage 3: RuntimeFROM mcr.microsoft.com/dotnet/aspnet:8.0 AS finalWORKDIR /appCOPY --from=publish /app/publish .ENTRYPOINT ["dotnet", "goldenCenterNew.dll"]EXPOSE 8080